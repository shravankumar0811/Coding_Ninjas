   "source": [
    "Number=int(input())\n",
    "even=0\n",
    "odd=0\n",
    "while(Number>0):\n",
    "    Reminder = Number %10\n",
    "    if(Reminder % 2 == 0):\n",
    "        even=even + Reminder\n",
    "    else:\n",
    "        odd= odd + Reminder\n",
    "    Number = Number //10\n",
    "print(even,\"\",odd)\n"
   ]
  }
