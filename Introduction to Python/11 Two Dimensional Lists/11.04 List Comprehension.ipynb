    "# You can also have multiple number of for loops\n",
    "li1 = [1,2,3,4,5]\n",
    "li2 = [2,4,6,7]\n",
    "li_inter = []\n",
    "for ele in li1:\n",
    "    for ele2 in li2:\n",
    "        if ele == ele2:\n",
    "            li_inter.append(ele)\n",
    "print(li_inter)"
   ]
