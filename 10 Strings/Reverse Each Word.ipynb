    "def reverseword(s): \n",
    "   \n",
    "   w = s.split(\" \")        \n",
    "                           \n",
    "   nw= [i[::-1] for i in w]\n",
    "                          \n",
    "   ns = \" \".join(nw)\n",
    "   return ns\n",
    "# Driver's Code \n",
    "s = input()\n",
    "print(reverseword(s))"
   ]
  }
